/*mfadt style*/
body {
	overflow-x: hidden;
-webkit-transition: all 100ms cubic-bezier(0.175, 0.885, 0.320, 1.275); 
   -moz-transition: all 100ms cubic-bezier(0.175, 0.885, 0.320, 1.275); 
     -o-transition: all 100ms cubic-bezier(0.175, 0.885, 0.320, 1.275);
	transition: 0.1s; 
        transition: all 100ms cubic-bezier(0.175, 0.885, 0.320, 1.275); /* easeOutBack */

-webkit-transition-timing-function: cubic-bezier(0.175, 0.885, 0.320, 1); /* older webkit */
-webkit-transition-timing-function: cubic-bezier(0.175, 0.885, 0.320, 1.275); 
   -moz-transition-timing-function: cubic-bezier(0.175, 0.885, 0.320, 1.275); 
     -o-transition-timing-function: cubic-bezier(0.175, 0.885, 0.320, 1.275); 
        transition-timing-function: cubic-bezier(0.175, 0.885, 0.320, 1.275); /* easeOutBack */
	/*background: #bfbfbf;*/
	-webkit-transition: all 100ms cubic-bezier(0.175, 0.885, 0.320, 1); /* older webkit */
        
}

.bodyZoom {
	-webkit-transform: scale(1.1) !important;
	transform: scale(1.1) !important;
}

.columns {
	overflow: hidden;
}

.hero {
	position: absolute;
	top: 0px;
	left: 0;
	z-index: 0;
	margin-top: 15px;
	width: 100%;
	height: 73%;
	background: url(../img/hero/handandheart.png) no-repeat;
	background-position: 0px 70px;
	background-size: 85%;
}

#hero-background {
	position:absolute;
	top:0; left:0;
	width: 100%;
	height: 100%;
	background: url(../img/hero/bg.png) no-repeat;
	background-size: cover;
	background-position: center;
}

.hero-information {
	position: absolute;
	right: 15px;
	bottom: 45%;
	color: #fff;
	font-size: 1.5em;
	line-height: 1.3em;
	/*text-shadow: 1px 1px 0px white;*/
	/*box-shadow: 10px 10px 0px black;*/
	/*background: rgba(255,255,255,0.8);*/
	padding: 20px;
}

.hero-information button {
	float:right;
	margin:0px;
	padding: 5px 15px 5px 15px;
	border: 1px solid black;
	border-radius: 0px;
	background: none;
	font-weight: 400;
	font-style: italic;
	font-size: 0.7em;
}

.hero-logo {
	margin: 30px;
	/*-webkit-filter: invert(100%);*/
}

.below-hero {
	padding: 0px;
}

.below-hero-content h6 {
	margin: 0px 0px 10px 0px;
	padding: 10px 0px 3px 0px;
	border-bottom: 2px solid #969696;
	color: #969696;
	text-transform: uppercase;
	font-weight: 700;
}

.noBottom {
	border:none !important;
}

/*header styles*/
#blurPanel {
	position: fixed;
	top: -100px; left: 0px;
	z-index: -999;
	overflow: hidden;
	width: 100%;
	height: 300%;
	opacity: 0;
	transition: 0.25s opacity !important;
}

.blurOnFront {
	z-index: 1 !important;
	opacity: 1 !important;
}

nav {
	width: 100%;
	height: 60px;
	border-bottom: 1px solid #c0c0c0;
	transition: opacity 0.3s;
	/*opacity: 0;*/
	background: rgba(255,255,255,1);
}

.menu-logo {
	float: left;
	margin-left: -500px;
	width: 170px;
	opacity: 0;
	transition: 0.25s;
}

.topMenu {
	margin: 0px;
	padding: 15px 0px 0px 0px;
	min-height: 60px;
}

.topMenu .menu {
	float: left;
	margin-left: 30px;
}

.topMenu form {
	float:right;
	margin-bottom: 0px;
}

.topMenu form {
	float:right;
	margin-bottom: 0px;
}

#searchsubmit {
	display: none;
}

#s {
	padding-left: 30px;
	background: url(../img/nav/search-128.png) no-repeat !important;
	background-size: contain !important;
	transition: 0s;
}

form::-webkit-input-placeholder {
    color: #7a7a7a;
}

.placeWhite::-webkit-input-placeholder {
    color: #fff;
}

.isSearching {
	position: absolute;
	top: 250px;
	left: 10%;
	z-index: 1;
	padding-left: 100px !important;
	width: 80% !important;
	color: #000 !important;
	font-size: 4.5em !important;
	transition: 0.3s width !important;
}

.topMenu form label {
	display: none;
}

.menu-main-container ul  {
	margin: 3px 0px 0px 0px;
}

.menu-main-container ul > li {
	display: inline-block;
	margin-right: 30px;
	text-transform: uppercase;
	font-weight: 600;
}

/*mainContainer styles*/

.mainContainer {
	margin: 0px 0px 15px 0px;
}

.studentInfoHolder{
	height: 150px;	
}

.studentBio{
	display: inline-block;
	width: 60%;
	vertical-align:text-top;
}

div.studentContactInfo{
	display: inline-block;
	overflow: hidden;
	margin: 0 30px 0 80px;
	width: 46px;
	vertical-align:text-top;
/* 	float: right; */

}
img.headshotRegular{
	display: inline-block;
	float: right;
	height:100%;
	vertical-align:text-top;
}

.personalWebsiteLink{
	margin-bottom: 2px;
	width: 46px;
	height: 46px;
	background-image: url(../img/icons/personalIcon.png);
	background-size: contain;
	-webkit-transition: background-image .95s ease-in-out;
	-moz-transition: background-image .95s ease-in-out;
	-o-transition: background-image .95s ease-in-out;
	transition: background-image .95s ease-in-out;
}

.personalWebsiteLink:hover{
	background-image: url(../img/icons/personalIconHover.png);
}

.linkedInLink{
	margin-bottom: 2px;
	width: 46px;
	height: 46px;
	background-image: url(../img/icons/linkedInIcon.png);
	background-size: contain;
	-webkit-transition: background-image .95s ease-in-out;
	-moz-transition: background-image .95s ease-in-out;
	-o-transition: background-image .95s ease-in-out;
	transition: background-image .95s ease-in-out;
}

.linkedInLink:hover{
	background-image: url(../img/icons/linkedInIconHover.png);
}

.twitterLink{
	width: 46px;
	height: 46px;
	background-image: url(../img/icons/twitterIcon.png);
	background-size: contain;
	-webkit-transition: background-image .95s ease-in-out;
	-moz-transition: background-image .95s ease-in-out;
	-o-transition: background-image .95s ease-in-out;
	transition: background-image .95s ease-in-out;
}

.twitterLink:hover{
	background-image: url(../img/icons/twitterIconHover.png);
}

